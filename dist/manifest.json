{
  "manifest_version": 2,
  "name": "Script Runner",
  "description": "Extensions to execute arbitrary JavaScript.",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon/js-logo.png",
    "default_popup": "popup.html"
  },
  
  "icons": {
    "16": "icon/js-logo.png"
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ]
}
