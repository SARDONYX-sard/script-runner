{
  "manifest_version": 2,
  "name": "Script Runner",
  "description": "Extensions to execute arbitrary JavaScript.",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon/js-logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/eventPage.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon/js-logo.png"
  },
  "permissions": [
    "storage"
  ]
}
